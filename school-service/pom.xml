      <artifactId>school-dao</artifactId>
      <version>1.0.0-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>com.yckj.school</groupId>
      <artifactId>school-common</artifactId>
      <version>1.0.0-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>com.yckj.common</groupId>
      <artifactId>common-fastdfs</artifactId>
      <version>1.0.0-SNAPSHOT</version>
